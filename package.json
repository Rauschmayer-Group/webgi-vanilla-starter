{
  "name": "webgi-vanilla-typescript",
  "version": "1.0.0",
  "description": "WebGi + TypeScript example starter project",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "b": "vite build --config=vite.config.prod.js && node fix_script_tag.js",
    "build": "vite build --config=vite.config.prod.js && node fix_script_tag.js",
    "preview": "vite preview",
    "s": "npm run build && cd ./dist && start http://localhost:8000 && py -m http.server"
  },
  "devDependencies": {
    "@types/webgi": "https://dist.pixotronics.com/webgi/runtime/bundle-types-0.11.1.tgz",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "webgi": "https://dist.pixotronics.com/webgi/runtime/bundle-0.11.1.tgz"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "typescript",
    "javascript"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "assets",
        "staticOutDir": "assets"
      }
    ]
  },
  "type": "module"
}
